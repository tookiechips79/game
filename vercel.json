{
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "outputDirectory": "dist",
  "env": {
    "VITE_API_URL": "@vite-api-url"
  },
  "routes": [
    {
      "src": "^/(?:.*)\\.(?:js|css|png|jpg|gif|svg|ico|webp|woff|woff2)$",
      "headers": {
        "cache-control": "public, immutable, max-age=31536000"
      },
      "dest": "/$0"
    },
    {
      "src": "^/.*",
      "dest": "/index.html"
    }
  ]
}
